    "name": "kris"
  },
  "title": {
    "defaultTitle": "mtab",
    "dynamic": {
      "enabled": true
    },
    "faviconType": "custom"
  },
  "message": {
    "enabled": true,
    "font": {
      "type": "custom",
      "custom": "https://fonts.googleapis.com/css2?family=Press+Start+2P&display=swap"
