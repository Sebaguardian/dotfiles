    "filters": {
      "brightness": "0.7",
      "blur": "0px"
    }
  },
  "ui": {
    "style": "glass",
    "glassColor": "#19172490",
    "blurStrength": "16px",
    "foregroundColor": "#191724",
    "backgroundColor": "#191724",
    "highlightColor": "#ffffff20",
    "cornerStyle": "round",
    "customCSS": "/* input custom css... */"
